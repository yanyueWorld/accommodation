<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="mysql" targetRuntime="Mybatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/accommodation"
                        userId="root" password="root">
        </jdbcConnection>

        <!--存放生成实体类的路径-->
        <javaModelGenerator targetPackage="com.example.yanyue.pojo" targetProject="src/main/java"/>
        <!--生成sql映射文件的路径-->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources/mybatis"/>
        <!--生成dao接口的目录-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.yanyue.dao" targetProject="src/main/java"/>

        <table tableName="account" mapperName="AccountDao" domainObjectName="Account" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="account_role" mapperName="AccountRoleDao" domainObjectName="AccountRole" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="apartment_attr" mapperName="ApartmentAttrDao" domainObjectName="ApartmentAttr" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="apartment_class" mapperName="ApartmentClassDao" domainObjectName="ApartmentClass" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="apartment_pic" mapperName="ApartmentPicDao" domainObjectName="ApartmentPic" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="appartment_type" mapperName="AppartmentTypeDao" domainObjectName="AppartmentType" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="appartment" mapperName="AppartmentDao" domainObjectName="Appartment" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="attr_val" mapperName="AttrValDao" domainObjectName="AttrVal" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="information_check_in" mapperName="InformationCheckInDao" domainObjectName="InformationCheckIn" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="message" mapperName="MessageDao" domainObjectName="Message" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="order" mapperName="OrderDao" domainObjectName="Order" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="picture" mapperName="PictureDao" domainObjectName="Picture" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="resource" mapperName="ResourceDao" domainObjectName="Resource" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="role" mapperName="RoleDao" domainObjectName="Role" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="role_resource" mapperName="RoleResourceDao" domainObjectName="RoleResource" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
    </context>
</generatorConfiguration>